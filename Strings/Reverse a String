#include <iostream>
#include <cstring> 
using namespace std;

class Solution{
    bool reverseString(char s[])
    {
        int len = strlen(s); 
        for (int i = 0; i < len / 2; i++)
        {
            swap(s[i], s[len-i-1]);
        }
    }
};


class Solution {
public:
    void reverseString(vector<char>& s) 
    {
        int len = s.size(); // size is inbuilt func
        for(int i = 0; i < len / 2; i++)
        {
            std::swap(s[i], s[len-i-1]);
        }
    }
};

//Above problem is solved on leetcode.


